{
  "version": 2,
  "builds": [
    {
      "src": "project-manager/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/project-manager/$1"
    }
  ]
}
